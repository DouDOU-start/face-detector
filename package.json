{
  "name": "face-detector-lite",
  "version": "1.2.0",
  "private": false,
  "description": "Lightweight browser face detector (TF.js + BlazeFace) with Vue 3 support",
  "keywords": [
    "face",
    "detector",
    "tensorflow",
    "blazeface",
    "browser",
    "vue3"
  ],
  "license": "MIT",
  "sideEffects": false,
  "main": "face-detector.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "require": "./face-detector.js",
      "default": "./face-detector.js",
      "types": "./types/index.d.ts"
    },
    "./vue": {
      "import": "./vue/index.js",
      "default": "./vue/index.js",
      "types": "./types/vue/index.d.ts"
    }
  },
  "files": [
    "face-detector.js",
    "dist/",
    "models/blazeface/embedded.js",
    "types/",
    "vue/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "node build.js",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "javascript-obfuscator": "^4.1.1",
    "terser": "^5.44.0"
  }
}
